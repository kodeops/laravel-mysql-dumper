{
    "name": "kodeops/laravel-mysql-dumper",
    "type": "library",
    "description": "Dump and restore the contents of a MySQL database",
    "keywords": ["kodeops", "laravel", "mysql", "dump"],
    "homepage": "https://github.com/kodeops/laravel-mysql-dumper",
    "license": "MIT",
    "authors": [
        {
            "name": "kodeops",
            "email": "hello@kodeops.io",
            "homepage": "https://kodeops.io",
            "role": "developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "spatie/laravel-package-tools": "^1.6.2",
        "ifsnop/mysqldump-php": "^2.7",
        "league/flysystem-aws-s3-v3": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "kodeops\\LaravelMysqlDumper\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "kodeops\\LaravelMysqlDumper\\LaravelMysqlDumperServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
